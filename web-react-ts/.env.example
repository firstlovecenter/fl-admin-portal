# Frontend Environment Variables
# Copy this file to .env.local for local development
# 
# ⚠️  IMPORTANT: In production, these are managed in AWS Secrets Manager
# Only VITE_* prefixed variables are exposed to the frontend by Vite

# Custom Authentication API
VITE_AUTH_API_URL=https://your-auth-service-url

# GraphQL API Endpoint
# Dev: https://dev-api-synago.firstlovecenter.com/graphql
# Prod: https://api-synago.firstlovecenter.com/graphql
# Fallback: http://localhost:4001/graphql
VITE_SYNAGO_GRAPHQL_URI=https://dev-api-synago.firstlovecenter.com/graphql

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
